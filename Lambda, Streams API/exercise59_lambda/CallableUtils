import java.util.ArrayList;
import java.util.List;

public class CallableUtils {
    public static String generateSum() {
        List<Integer> random = new ArrayList<>();
        for (int j = 0; j < 10; j++) {
            random.add((int) (Math.random() * 20));
        }
        System.out.println(Thread.currentThread().getName() + random);
        try {
            Thread.sleep((int) (1000 + Math.random() * 9000));
        } catch (InterruptedException e) {
            e.printStackTrace();
        }
        Integer sum = 0;
        for (Integer x : random) {
            sum += x;
        }
        return sum.toString();
    }
}
